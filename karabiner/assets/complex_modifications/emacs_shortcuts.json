{
  "title": "No minimize",
  "rules": [
    {
      "description": "Emacs Shortcuts",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": { "mandatory": ["command"] }
          },
          "to": [{
            "key_code": "m",
            "modifiers": ["left_command", "left_shift", "left_control"]
          }],
          "type": "basic"
        }
      ]
    }
  ]
}
